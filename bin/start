#!/bin/sh

git pull origin main
echo "############# GIT OK #############"
workon goeat_env
echo "############# WORKON OK #############"
killall -9 uwsgi
echo "############# KILL OK #############"
uwsgi --ini /usr/local/share/GoEat-Docker/goeat/goeat_uwsgi.ini
echo "############# START OK #############"
tail -111f /usr/local/share/GoEat-Docker/goeat/logs/uwsgi.log